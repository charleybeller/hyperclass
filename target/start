#!/bin/bash

function die() {
    echo "$*" 1>&2
    exit 1
}
test -x './target/start' || die "'./target/start' not found, this script must be run from the project base directory"


MAINCLASS="$1"
shift
if test x"$MAINCLASS" = x; then
    die 'This "start" script requires a main class name as the first argument, because a mainClass was not specified in SBT and not autodetected by SBT (usually means you have zero, or more than one, main classes).  You could specify in your SBT build: mainClass in Compile := Some("Whatever")'
fi



exec java $JAVA_OPTS -cp "./target/scala-2.10/classes:./lib/vtd-xml-2.10.jar:./lib/liblinear-1.92.jar:./lib/agiga-1.0.jar:./lib/jerboa.jar:/home/hltcoe/epavlick/.sbt/boot/scala-2.10.0/lib/scala-library.jar:./lib_managed/jars/commons-lang/commons-lang/commons-lang-2.6.jar:./lib_managed/bundles/log4j/log4j/log4j-1.2.17.jar:./lib_managed/jars/edu.stanford.nlp/stanford-corenlp/stanford-corenlp-1.3.4.jar:./lib_managed/jars/xom/xom/xom-1.2.5.jar:./lib_managed/jars/xml-apis/xml-apis/xml-apis-1.3.03.jar:./lib_managed/jars/xerces/xercesImpl/xercesImpl-2.8.0.jar:./lib_managed/jars/xalan/xalan/xalan-2.7.0.jar:./lib_managed/jars/joda-time/joda-time/joda-time-2.1.jar:./lib_managed/jars/de.jollyday/jollyday/jollyday-0.4.7.jar:./lib_managed/jars/javax.xml.bind/jaxb-api/jaxb-api-2.2.7.jar:./lib_managed/jars/edu.mit/jwi/jwi-2.2.3.jar:./lib_managed/jars/org.scalanlp/breeze-math_2.10/breeze-math_2.10-0.2.3.jar:./lib_managed/jars/org.scalanlp/breeze-core_2.10/breeze-core_2.10-0.2.3.jar:./lib_managed/jars/com.thoughtworks.paranamer/paranamer/paranamer-2.2.jar:./lib_managed/jars/net.sf.opencsv/opencsv/opencsv-2.3.jar:./lib_managed/jars/com.typesafe/scalalogging-log4j_2.10/scalalogging-log4j_2.10-1.0.1.jar:./lib_managed/jars/org.scala-lang/scala-reflect/scala-reflect-2.10.0.jar:./lib_managed/jars/org.apache.logging.log4j/log4j-core/log4j-core-2.0-beta4.jar:./lib_managed/jars/org.apache.logging.log4j/log4j-api/log4j-api-2.0-beta4.jar:./lib_managed/jars/com.googlecode.netlib-java/netlib-java/netlib-java-0.9.3.jar:./lib_managed/jars/net.sourceforge.f2j/arpack_combined_all/arpack_combined_all-0.1.jar:./lib_managed/jars/org.jblas/jblas/jblas-1.2.3.jar:./lib_managed/jars/org.scalanlp/breeze-learn_2.10/breeze-learn_2.10-0.2.3.jar:./lib_managed/jars/org.scalanlp/breeze-process_2.10/breeze-process_2.10-0.2.3.jar:./lib_managed/jars/de.bwaldvogel/liblinear/liblinear-1.92.jar:./lib_managed/jars/org.scalanlp/breeze-viz_2.10/breeze-viz_2.10-0.2.3.jar:./lib_managed/jars/jfree/jcommon/jcommon-1.0.16.jar:./lib_managed/jars/jfree/jfreechart/jfreechart-1.0.13.jar:./lib_managed/jars/org.apache.xmlgraphics/xmlgraphics-commons/xmlgraphics-commons-1.3.1.jar:./lib_managed/jars/commons-io/commons-io/commons-io-1.3.1.jar:./lib_managed/jars/commons-logging/commons-logging/commons-logging-1.0.4.jar:./lib_managed/jars/com.lowagie/itext/itext-2.1.5.jar:./lib_managed/jars/bouncycastle/bcmail-jdk14/bcmail-jdk14-138.jar:./lib_managed/jars/bouncycastle/bcprov-jdk14/bcprov-jdk14-138.jar" "$MAINCLASS" "$@"

